@prefix : <https://w3id.org/agent-ontology/economic#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<https://w3id.org/agent-ontology/economic>
  dcterms:title "Economic Ontology" ;
  dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
  dcterms:rightsHolder "Semantic Agent Communication Community Group" ;
  dcterms:creator "Tyson Chen (SlashLife AI)" ;
  dcterms:description "Defines concepts related to economic aspects of an AI Agent, such as wallets." ;
  owl:versionInfo "0.1.0" .

#################################################################
#   Economic Interfaces
#################################################################

:WalletSet a owl:Class ;
    rdfs:label "WalletSet" .

:Wallet a owl:Class ;
    rdfs:label "Wallet" .

:wallet a owl:ObjectProperty ;
    rdfs:domain :WalletSet ;
    rdfs:range :Wallet .

:address a owl:DatatypeProperty ;
    rdfs:domain :Wallet ;
    rdfs:range xsd:string .