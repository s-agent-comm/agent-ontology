{
  "@context": {
    "@vocab": "https://w3id.org/agent-ontology/lifecycle#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "LifecycleRecord": "lifecycle:LifecycleRecord",
    "GenesisRecord": "lifecycle:GenesisRecord",
    "HealthCheckSpec": "lifecycle:HealthCheckSpec",
    "DecommissionRecord": "lifecycle:DecommissionRecord",

    "genesisRecord": {
      "@id": "lifecycle:genesisRecord",
      "@type": "@id"
    },
    "state": {
      "@id": "lifecycle:state",
      "@type": "xsd:string"
    },
    "healthCheck": {
      "@id": "lifecycle:healthCheck",
      "@type": "@id"
    },
    "decommissionRecord": {
      "@id": "lifecycle:decommissionRecord",
      "@type": "@id"
    }
  }
}