@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix agent-profile: <https://w3id.org/agent-ontology/agent-profile#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# -- Shape for agent-profile:AgentProfile --
agent-profile:AgentProfileShape
    a sh:NodeShape ;
    sh:targetClass agent-profile:AgentProfile ;
    sh:property [
        sh:path agent-profile:hasPersonality ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path agent-profile:usesLanguage ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path agent-profile:hasToneModel ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:nodeKind sh:IRI ;
    ] .