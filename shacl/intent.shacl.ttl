@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix intent: <https://w3id.org/agent-ontology/intent#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# -- Shape for intent:Intent --
intent:IntentShape
    a sh:NodeShape ;
    sh:targetClass intent:Intent ;
    sh:property [
        sh:path intent:hasIntentType ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:nodeKind sh:IRI ;
    ] .
